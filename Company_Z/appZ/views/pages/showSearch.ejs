<h1 class ="display-4" style="text-align: center;">Search</h1>
<div style="width:700px; margin:auto">
    <table class = "table table-striped">
        <thead class="thead-dark">
            <tr>
                <th>Job Name</th>
                <th>Date</th>
                <th>Time</th>
            </tr>
        </thead>
        <tbody>
            <% search17.forEach(function(search){ %>
                <tr>
                    <td><%= search.id %></td>
                    <td><%= search.date %></td>
                    <td><%= search.time %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
    <form action="/" method="GET" class="m-5">
        <center> <button class="btn btn-primary"> Home </button> </center>
    </form>
</div>